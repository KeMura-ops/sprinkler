.sec-container {
  height: 800px;
  color: #fffafa;
}

.free-title {
  font-size: 38px;
  font-weight: bold;
  padding-top: 15px;
  padding-left: 16px;
  margin-bottom: 30px;
}

.free-text {
  font-size: 24px;
  font-weight: bold;
  padding-top: 128px;
  padding-left: 16px;
}

.free-section {
  background-image: url("<%= asset_path('free-section.png') %>");
  background-color: #483d8b;
  background-blend-mode: hard-light;
  background-position: center center;
  background-size: cover;
}